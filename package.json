{
  "private": true,
  "name": "laravel-livewire-debugger",
  "description": "Livewire Debugger using VueJs devtools",
  "version": "0.1.1",
  "scripts": {
    "local": "cross-env NODE_ENV=development NODE_OPTIONS=--openssl-legacy-provider node_modules/webpack/bin/webpack.js --progress --env=local --config=node_modules/laravel-mix/setup/webpack.config.js",
    "staging": "cross-env NODE_ENV=staging node_modules/webpack/bin/webpack.js --progress --env=staging --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --env=production --config=node_modules/laravel-mix/setup/webpack.config.js",
    "dev": "npm run local",
    "prod": "npm run production",
    "watch": "npm run local -- --watch"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.5.16",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "file-loader": "^6.2.0",
    "laravel-mix": "^6.0.11",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.2.13",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.32.5",
    "sass-loader": "^10.1.1",
    "vue": "^3.5.16",
    "vue-loader": "^15.9.6"
  }
}
