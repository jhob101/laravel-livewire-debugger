{
    "name": "Livewire Debugger",
    "manifest_version": 3,
    "version": "0.2.1",
    "description": "Debug Livewire in Vue.js devtools",
    "icons": {
        "16": "images/livewire.png",
        "32": "images/livewire.png",
        "64": "images/livewire.png",
        "128": "images/livewire.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["js/content.js"],
            "css": ["css/style.css"],
            "world": "MAIN"
        }
    ]
}
